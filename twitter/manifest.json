{
	"name": "Twitter One-Click Search",
	"description": "Add Twitter to your One-Click search engines. Lovingly forked from https://dannycolin.com",
	"version": "0.1.1",
	"manifest_version": 2,
	"applications":{
		"gecko": {
			"id": "twitter@search.dannycolin.com",
			"strict_min_version": "64.0"
		}
	},
	"developer": {
		"name": "Eli Itah",
		"url": "https://www.that-eli.com/"
	},
	"icons": {
		"16": "favicon.png"
	},
	"web_accessible_resources": [
		"favicon.png"
	],
  "chrome_settings_overrides": {
		"search_provider": {
			"name": "Twitter",
      "keyword": "@tw",
			"search_url": "https://twitter.com/search?q={searchTerms}"
		}
	}
}

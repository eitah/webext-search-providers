{
	"name": "Itah GitHub Search",
	"description": "Add GitHub to your search engine. Lovingly forked from https://dannycolin.com",
	"version": "0.2",
	"manifest_version": 2,
	"applications":{
		"gecko": {
			"id": "github@search.dannycolin.com",
			"strict_min_version": "64.0"
		}
	},
	"developer": {
		"name": "Eli Itah",
		"url": "https://www.that-eli.com/"
	},
	"icons": {
		"16": "favicon.png"
	},
	"web_accessible_resources": [
		"favicon.png"
	],
  "chrome_settings_overrides": {
		"search_provider": {
			"name": "GitHub",
      "keyword": "@gh",
			"search_url": "https://github.com/search?q=org:tempuslabs+{searchTerms}"
		}
	}
}

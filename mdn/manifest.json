{
	"name": "Moz Developer Network (MDN) Search",
	"description": "Add Mozilla Developer Network (MDN) to your search engine. Lovingly forked from https://dannycolin.com",
	"version": "0.1",
	"manifest_version": 2,
	"applications":{
		"gecko": {
			"id": "mdn@search.dannycolin.com",
			"strict_min_version": "64.0"
		}
	},
	"developer": {
		"name": "Eli Itah",
		"url": "https://www.that-eli.com/"
	},
	"icons": {
		"32": "favicon.png"
	},
	"web_accessible_resources": [
		"favicon.png"
	],
  "chrome_settings_overrides": {
		"search_provider": {
			"name": "Mozilla Developer Network (MDN)",
      "keyword": "@mdn",
			"search_url": "https://developer.mozilla.org/search?q={searchTerms}"
		}
	}
}
